pipeline {
         agent any
         stages {
                 stage('One') {
                 steps {
                     python firstjob.py
                 }
                 }
                 stage('Two') {
                 steps {
                    input('Do you want to proceed?')
                 }
                 }
                 stage('Three') {
                 when {
                       not {
                            branch "master"
                       }
                 }
                 steps {
                       Javac Hello.java
                 }
                 }
                 stage('Four') {
                 parallel { 
                            stage('Unit Test') {
                           steps {
                                echo "Running the unit test..."
                           }
                           }
                            stage('Integration test') {
                           
                              steps {
                                echo "Running the integration test..."
                              }
                           }
                           }
                           }
              }
}

